{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby boilerplate",
  "author": "Piotr Krasny",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "start": "npm run develop",
    "serve": "cross-env NODE_OPTIONS=\"-r esm\" gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^3.2.1",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "gatsby-transformer-sharp": "^3.2.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-source-sanity": "^7.0.2",
    "gatsby-source-cloudinary": "^0.1.14",
    "gatsby-remark-responsive-iframe": "^3.2.0",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-remark-copy-linked-files": "^3.2.0",
    "gatsby-plugin-styled-components": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "esm": "^3.2.25",
    "babel-eslint": "^10.1.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.1"
  }
}
